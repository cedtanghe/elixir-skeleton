{
    "name": "elixir/elixir-skeleton",
    "description": "Skeleton for Elixir application",
    "license":"proprietary",
    "homepage": "https://github.com/cedtanghe/elixir-skeleton",
    "repositories": [
        {
        "type": "vcs",
        "url": "https://github.com/cedtanghe/elixir"
        }
    ],
    "config": {
        "vendor-dir": "application/vendor"
    },
    "require": {
        "cedtanghe/elixir": "dev-master"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.0",
        "symfony/console": "2.5.*@dev"
    },
    "autoload": {
        "psr-4": {
            "App\\": "application/modules/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Site\\Test\\": "tests/"
        }
    }
}
